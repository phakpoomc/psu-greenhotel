<!DOCTYPE html>
<html>
    <head>
        <%- include('../partials/formhead'); %>
        <script>
            window.onbeforeprint = (event) => {
                document.getElementById("sidenav").classList.add("custom-hide");
                document.getElementById("topnav").classList.add("custom-hide");
            }

            window.onafterprint = (event) => {
                document.getElementById("sidenav").classList.remove("custom-hide");
                document.getElementById("topnav").classList.remove("custom-hide");
            };
        </script>
        
    </head>

    <body onkeypress="checkPrint(event)" style="font-size: 1.25em;">
        <%- include('../partials/navbar'); %>
        <%- include('../partials/user/sidebar', {name: username, profilepic: "https://github.com/mdo.png", data: data}); %>

       
    </body>
</html>